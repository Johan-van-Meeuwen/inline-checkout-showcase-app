{
    "version": 2,
    "functions": {
        "app.js": {
            "maxDuration": 30
        }
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "routes": [
        {
            "src": "/settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-checkout-settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-prices",
            "dest": "/app.js"
        },
        {
            "src": "/env",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "public": true
}