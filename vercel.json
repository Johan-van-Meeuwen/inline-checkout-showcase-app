{
    "version": 2,
    "builds": [
        {
            "src": "/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-checkout-settings",
            "dest": "/app.js"
        },
        {
            "src": "/get-prices",
            "dest": "/app.js"
        },
        {
            "src": "/env",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "functions": {
        "app.js": {
            "maxDuration": 30
        }
    },
    "public": true
}